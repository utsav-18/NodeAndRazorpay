<script src="/js/newScript.js"></script>
<script>
  // ===== Hide Sticky CTA when Footer Appears (Desktop + Mobile) =====
(function () {
  const cta = document.getElementById("ucta_wrapper");
  const footer = document.querySelector("footer");

  if (!cta || !footer) return;

  const observer = new IntersectionObserver(
    (entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          // Footer visible ‚Üí hide CTA
          cta.style.opacity = "0";
          cta.style.transform = "translateY(40px)";
          cta.style.pointerEvents = "none";
        } else {
          // Footer not visible ‚Üí show CTA
          cta.style.opacity = "1";
          cta.style.transform = "translateY(0)";
          cta.style.pointerEvents = "auto";
        }
      });
    },
    {
      root: null,
      threshold: 0.12 // works well for mobile + desktop
    }
  );

  observer.observe(footer);
})();

</script>
