/* Base CTA */
.cta-strong {
  transition: 
    background 0.45s cubic-bezier(.25,.8,.25,1),
    transform 0.45s cubic-bezier(.2,.9,.2,1),
    box-shadow 0.45s ease;
  position: relative;
  overflow: hidden;
}

/* Motion Hover Effect */
.cta-strong:hover {
  background: #ab0000;
  transform: translateY(-6px) scale(1.03);
  box-shadow: 
    0 20px 40px rgba(83, 3, 3, 0.35),
    0 10px 18px rgba(0,0,0,0.22);
  animation: ctaWiggle 0.45s ease-out forwards;
}

/* Active press */
.cta-strong:active {
  transform: scale(.96);
  box-shadow: 0 8px 20px rgba(0,0,0,0.25);
}

/* ‚ú® KEYFRAMES: subtle wiggle motion */
@keyframes ctaWiggle {
  0% { transform: translateY(-6px) scale(1.03) translateX(0); }
  25% { transform: translateY(-6px) scale(1.03) translateX(3px); }
  50% { transform: translateY(-6px) scale(1.03) translateX(-3px); }
  75% { transform: translateY(-6px) scale(1.03) translateX(2px); }
  100% { transform: translateY(-6px) scale(1.03) translateX(0); }
}

/* Optional: soft glow pulse while hovering */
.cta-strong:hover::after {
  content: "";
  position: absolute;
  inset: -6px;
  border-radius: inherit;
  background: radial-gradient(
    rgba(255, 60, 60, 0.3),
    transparent 70%
  );
  animation: ctaGlow 1.2s ease-out forwards;
  pointer-events: none;
}

@keyframes ctaGlow {
  0% { opacity: 0; transform: scale(0.6); }
  70% { opacity: 1; transform: scale(1.2); }
  100% { opacity: 0; transform: scale(1.4); }
}

